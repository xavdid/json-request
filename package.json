{
  "name": "@xavdid/json-requests",
  "version": "0.3.0",
  "description": "An extremely simple package for GETting and POSTing JSON.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "http requests",
    "json"
  ],
  "files": [
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "author": {
    "name": "David Brownman",
    "email": "beamneocube@gmail.com",
    "url": "https://xavd.id"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": "github:xavdid/json-requests",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@swc/core": "^1.3.27",
    "@swc/jest": "0.2.24",
    "@types/jest": "^29.2.6",
    "@types/node": "^20.11.30",
    "eslint": "^7",
    "eslint-config-xavdid": "^3.0.0",
    "jest": "^29.3.1",
    "nock": "^14.0.0-beta.5",
    "prettier": "^2.8.3",
    "tsd": "0.25.0",
    "typescript": "4.9.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
